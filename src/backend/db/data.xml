<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <RECORD>
        <FIELD ID="1" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="11" />
        <FIELD ID="2" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="10" />
        <FIELD ID="3" xsi:type="CharTerm" TERMINATOR="\n" MAX_LENGTH="11" />
    </RECORD>
    <ROW>
        <COLUMN SOURCE="1" NAME="Id" xsi:type="SQLINT" />
        <COLUMN SOURCE="2" NAME="Date" xsi:type="SQLDATE" />
        <COLUMN SOURCE="3" NAME="TemperatureC" xsi:type="SQLINT" />
    </ROW>
</BCPFORMAT>